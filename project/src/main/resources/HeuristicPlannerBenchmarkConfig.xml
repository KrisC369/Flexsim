<plannerBenchmark>
    <benchmarkDirectory>local/data/bench</benchmarkDirectory>
    <!--<parallelBenchmarkCount>AUTO</parallelBenchmarkCount>-->
    <warmUpSecondsSpentLimit>30</warmUpSecondsSpentLimit>
    <inheritedSolverBenchmark>
        <problemBenchmarks>
            <solutionFileIOClass>be.kuleuven.cs.flexsim.experimentation.tosg.benchmark.persistence.HeuristicSolverIO</solutionFileIOClass>
            <inputSolutionFile>/tmp/foo/model_a1_1.txt</inputSolutionFile>
            <inputSolutionFile>/tmp/foo/model_a1_2.txt</inputSolutionFile>
            <inputSolutionFile>/tmp/foo/model_a1_3.txt</inputSolutionFile>
            <!--<inputSolutionFile>be/kuleuven/cs/flexsim/experimentation/data/dummy.txt</inputSolutionFile>-->
            <!--<inputSolutionFile>be/kuleuven/cs/flexsim/experimentation/data/dummy.txt</inputSolutionFile>-->
            <problemStatisticType>BEST_SCORE</problemStatisticType>
            <problemStatisticType>STEP_SCORE</problemStatisticType>
            <problemStatisticType>MEMORY_USE</problemStatisticType>
            <problemStatisticType>MOVE_COUNT_PER_STEP</problemStatisticType>
            <problemStatisticType>CALCULATE_COUNT_PER_SECOND</problemStatisticType>

        </problemBenchmarks>
        <solver>
            <solutionClass>be.kuleuven.cs.flexsim.solver.heuristic.domain.Allocation</solutionClass>
            <entityClass>be.kuleuven.cs.flexsim.solver.heuristic.domain.ActivationAssignment</entityClass>
            <scoreDirectorFactory>
                <scoreDefinitionType>HARD_SOFT</scoreDefinitionType>
                <scoreDrl>be/kuleuven/cs/flexsim/solver/heuristic/solver/SolverScoreRules_FullSat.drl</scoreDrl>
                <!--<scoreDrl>org/optaplanner/examples/cloudbalancing/solver/cloudBalancingScoreRules.drl</scoreDrl>-->
            </scoreDirectorFactory>
            <termination>
                <!--<terminationCompositionStyle>OR</terminationCompositionStyle>-->
                <secondsSpentLimit>180</secondsSpentLimit>
                <!--<unimprovedSecondsSpentLimit>30</unimprovedSecondsSpentLimit>-->
            </termination>
            <!--<constructionHeuristic>-->
                <!--<constructionHeuristicType>FIRST_FIT</constructionHeuristicType>-->
            <!--</constructionHeuristic>-->
        </solver>
    </inheritedSolverBenchmark>
    <solverBenchmarkBluePrint>
        <solverBenchmarkBluePrintType>EVERY_CONSTRUCTION_HEURISTIC_TYPE_WITH_EVERY_LOCAL_SEARCH_TYPE</solverBenchmarkBluePrintType>
    </solverBenchmarkBluePrint>
    <parallelBenchmarkCount>AUTO</parallelBenchmarkCount>
